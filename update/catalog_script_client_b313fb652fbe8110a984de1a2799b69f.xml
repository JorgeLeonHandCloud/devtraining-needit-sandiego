	if(newValue.length > 8 && newValue.length < 15){
		g_form.hideFieldMsg("u_phone", true);
	}
	else {
		g_form.showFieldMsg("u_phone", "The phone should be greater than 8 and less than 15", "error");
	}
}]]></script>
